<body>

  <h1>游꿢 Caza el c칤rculo</h1>
  <div id="info">
    Puntos: <span id="score">0</span> |
    Tiempo: <span id="time">30</span>s
  </div>

  <div id="gameArea">
    <div id="item"></div>
  </div>

  <script>
    const item = document.getElementById("item");
    const gameArea = document.getElementById("gameArea");
    const scoreSpan = document.getElementById("score");
    const timeSpan = document.getElementById("time");

    let score = 0;
    let timeLeft = 30;

    function moverItem() {
      const maxX = gameArea.clientWidth - item.clientWidth;
      const maxY = gameArea.clientHeight - item.clientHeight;

      const x = Math.random() * maxX;
      const y = Math.random() * maxY;

      item.style.left = x + "px";
      item.style.top = y + "px";
    }

    item.addEventListener("click", () => {
      score++;
      scoreSpan.textContent = score;
      moverItem();
    });

    const timer = setInterval(() => {
      timeLeft--;
      timeSpan.textContent = timeLeft;

      if (timeLeft <= 0) {
        clearInterval(timer);
        item.style.display = "none";
        alert("춰Juego terminado! Puntuaci칩n final: " + score);
      }
    }, 1000);

    moverItem();
  </script>

